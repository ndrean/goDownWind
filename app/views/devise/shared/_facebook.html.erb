<div class="col-md-6 col-lg-4">
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class:"badge badge-primary", style:"font-size: 20px" %><br />
  <% end %>
<% end %>
<br>
</div>